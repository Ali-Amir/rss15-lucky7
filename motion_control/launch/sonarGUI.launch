<launch>
  <include file="$(find vision)/launch/config.launch" />

  <node name="lis" pkg="rosjava_bootstrap" type="run.py" args="uorc_listener Listener" machine="netbook" /> 
  <node name="pub" pkg="rosjava_bootstrap" type="run.py" args="uorc_publisher Publisher" machine="netbook" />
  <node name="odo" pkg="rosjava_bootstrap" type="run.py" args="odometry odometry.Odometry" machine="netbook" /> 
  <node name="nav" pkg="rosjava_bootstrap" type="run.py" args="motion_control LocalNavigation.LocalNavigation"     machine="workstation" output="screen" /> 
  <node name="gui" pkg="rosjava_bootstrap" type="run.py" args="motion_control LocalNavigation.SonarGUI" machine="workstation" output="screen" />
</launch>
